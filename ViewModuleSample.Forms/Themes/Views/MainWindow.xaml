<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:james="https://jamesnet.dev/xaml/presentation"
    xmlns:views="clr-namespace:ViewModuleSample.Forms.UI.Views">
    <Style BasedOn="{StaticResource {x:Type james:DarkThemeWindow}}" TargetType="{x:Type views:MainWindow}">
        <Setter Property="Title">
            <Setter.Value>
                <TextBlock Foreground="White" Text="Window" />
            </Setter.Value>
        </Setter>
        <Setter Property="Content">
            <Setter.Value>
                <james:JamesRegion RegionName="MainRegion" />
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>